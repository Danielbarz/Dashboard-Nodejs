generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// ... Sisanya model User, Product, dll ...

model DigitalProduct {
  id                  BigInt    @id @default(autoincrement())
  orderNumber         String?   @unique @map("order_number")
  productName         String?   @map("product_name")
  customerName        String?   @map("customer_name")
  poName              String?   @map("po_name")
  witel               String?
  regional            String?
  channel             String?
  branch              String?
  revenue             Decimal   @default(0) @db.Decimal(18, 2)
  amount              Decimal   @default(0) @db.Decimal(18, 2)
  netPrice            Decimal   @default(0) @map("net_price") @db.Decimal(18, 2)
  status              String?
  milestone           String?
  segment             String?
  category            String?
  subType             String?   @map("sub_type")
  orderDate           DateTime? @map("order_date")
  orderCreatedDate    DateTime? @map("order_created_date")
  billcompDate        DateTime? @map("billcomp_date")
  lastUpdate          DateTime? @map("last_update")
  batchId             String?   @map("batch_id")
  filterProduct       String?   @map("filter_product")
  productOrderId      String?   @map("product_order_id")
  ach                 String?
  activeUser          String?   @map("active_user")
  joinNamaTarget      String?   @map("join_nama_target")
  layanan             String?
  orderStatusN        String?   @map("order_status_n")
  productNameGroup    String?   @map("product_name_group")
  produkDetails       String?   @map("produk_details")
  regionalAntaresEazy String?   @map("regional_antares_eazy")
  regionalJoin        String?   @map("regional_join")
  segmenN             String?   @map("segmen_n")
  sto                 String?
  tahun               Int?
  week                Int?
  targetOperational   String?   @map("target_operational")
  telda               String?
  createdAt           DateTime  @default(now()) @map("created_at")
  updatedAt           DateTime  @updatedAt @map("updated_at")

  @@index([witel])
  @@index([regional])
  @@index([channel])
  @@index([branch])
  @@index([status])
  @@index([orderDate])
  @@map("digital_products")
}

model User {
  id                      BigInt                   @id @default(autoincrement())
  name                    String                   @db.VarChar(255)
  email                   String                   @unique @db.VarChar(255)
  role                    String                   @default("user") @db.VarChar(255)
  currentRoleAs           String?                  @map("current_role_as") @db.VarChar(255)
  emailVerifiedAt         DateTime?                @map("email_verified_at") @db.Timestamp(6)
  password                String                   @db.VarChar(255)
  rememberToken           String?                  @map("remember_token") @db.VarChar(100)
  createdAt               DateTime?                @map("created_at") @db.Timestamp(6)
  updatedAt               DateTime?                @updatedAt @map("updated_at") @db.Timestamp(6)
  customTargets           CustomTarget[]
  userTableConfigurations UserTableConfiguration[]

  @@map("users")
}

model AccountOfficer {
  id                  BigInt    @id @default(autoincrement())
  name                String    @db.VarChar(255)
  displayWitel        String    @map("display_witel") @db.VarChar(255)
  filterWitelLama     String    @map("filter_witel_lama") @db.VarChar(255)
  specialFilterColumn String?   @map("special_filter_column") @db.VarChar(255)
  specialFilterValue  String?   @map("special_filter_value") @db.VarChar(255)
  createdAt           DateTime? @map("created_at") @db.Timestamp(6)
  updatedAt           DateTime? @updatedAt @map("updated_at") @db.Timestamp(6)

  @@map("account_officers")
}

