model Target {
  id            BigInt   @id @default(autoincrement())
  dashboardType String   @default("DIGITAL") @map("dashboard_type") @db.VarChar(50)
  periodType    String?  @map("segment") @db.VarChar(50) 
  targetType    String   @map("metric_type") @db.VarChar(50)
  witel         String   @map("nama_witel") @db.VarChar(100)
  product       String   @map("product_name") @db.VarChar(100)
  value         Decimal  @map("target_value") @db.Decimal(18, 2)
  periodDate    DateTime @map("period") @db.Date
  createdAt     DateTime @default(now()) @map("created_at") @db.Timestamp(6)
  updatedAt     DateTime @updatedAt @map("updated_at") @db.Timestamp(6)

  @@map("targets")
}

model list_po {
  id         BigInt    @id @default(autoincrement())
  nipnas     String?
  po         String
  segment    String?
  bill_city  String?
  witel      String?
  created_at DateTime? @default(now()) @map("created_at")
  updated_at DateTime? @updatedAt @map("updated_at")

  @@map("list_po")
}

model CustomTarget {
  id        BigInt    @id @default(autoincrement())
  userId    BigInt    @map("user_id")
  pageName  String    @map("page_name") @db.VarChar(255)
  targetKey String    @map("target_key") @db.VarChar(255)
  witel     String    @db.VarChar(255)
  period    DateTime  @db.Date
  value     Decimal   @default(0.00) @db.Decimal(15, 2)
  createdAt DateTime? @map("created_at") @db.Timestamp(6)
  updatedAt DateTime? @updatedAt @map("updated_at") @db.Timestamp(6)
  user      User      @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_custom_targets_users")

  @@map("custom_targets")
}

model UserTableConfiguration {
  id            BigInt    @id @default(autoincrement())
  userId        BigInt?   @map("user_id")
  pageName      String    @unique @map("page_name") @db.VarChar(255)
  configuration Json
  createdAt     DateTime? @map("created_at") @db.Timestamp(6)
  updatedAt     DateTime? @updatedAt @map("updated_at") @db.Timestamp(6)
  user          User?     @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_user_table_configs_user")

  @@map("user_table_configurations")
}

model cache {
